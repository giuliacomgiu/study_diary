<div class = "row p-2 mx-2">
  <h3><%= status.capitalize %></h3>
  <%= render partial: 'order_by', locals: { study_item: @study_item } %>
</div>
<div class = "row p-2 mx-2">
  <% @study_items.each do |study_item| %>
    <% if study_item.status.eql? status %>
      <div class = "col-3 border p-3 mx-3 ">
      <%= render partial: "study_item_preview", locals: { study_item: study_item } %>
      </div>
    <% end %>
  <% end %>
</div>